{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules",
        "dist"
    ],
    "env": {
        "NODE_ENV": "development",
        "TS_NODE_PROJECT": "./tsconfig.json"
    },
    "watch": [
        "src/"
    ],
    "ext": "ts,tsx",
    "exec": "ts-node ./src/index.ts"
}